{
  "name": "@tolani/debo",
  "version": "0.1.0",
  "description": "DEBO - Autonomous AI software engineer for Slack & Discord - responds to mentions, executes code changes, and creates PRs",
  "type": "module",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": false,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tolani-Corp/DevBot.git"
  },
  "homepage": "https://github.com/Tolani-Corp/DevBot#readme",
  "bugs": {
    "url": "https://github.com/Tolani-Corp/DevBot/issues"
  },
  "keywords": [
    "ai",
    "agent",
    "autonomous",
    "slack",
    "discord",
    "code-generation",
    "pull-request",
    "devops",
    "security"
  ],
  "main": "dist/sdk.js",
  "types": "dist/sdk.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sdk.d.ts",
      "import": "./dist/sdk.js"
    },
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "import": "./dist/agents/index.js"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "import": "./dist/ai/index.js"
    },
    "./server": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "debo": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "worker": "tsx watch src/worker.ts",
    "worker:prod": "node dist/worker.js",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:onboarding": "tsx tests/onboarding.test.ts",
    "pentest": "tsx scripts/pentest-demo.ts",
    "test:kali-mcp": "tsx scripts/test-kali-mcp.ts",
    "show-reasoning": "tsx scripts/show-reasoning.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@octokit/rest": "^21.0.2",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.203.0",
    "@opentelemetry/instrumentation": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@slack/bolt": "^4.1.0",
    "@slack/web-api": "^7.8.0",
    "@traceloop/instrumentation-anthropic": "^0.14.4",
    "bullmq": "^5.34.0",
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.0",
    "glob": "^13.0.1",
    "ioredis": "^5.4.2",
    "nanoid": "^5.0.9",
    "openai": "^6.19.0",
    "playwright": "^1.58.2",
    "postgres": "^3.4.8",
    "pptxgenjs": "^3.12.0",
    "stripe": "^20.3.1",
    "twitter-api-v2": "^1.29.0",
    "ws": "^8.19.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/ws": "^8.18.1",
    "drizzle-kit": "^0.30.0",
    "prettier": "^3.4.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
